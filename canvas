<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        canvas{
            border: 1px solid;
        }
    </style>
</head>
<body>
    <canvas id="name" width="400" height="400"> </canvas>
    <script>
        var canvas=document.getElementById('name');
        var c=canvas.getContext('2d');
        c.fillStyle="red";
        c.fillRect(30,50,100,60);
        c.fillStyle="blue";
        c.fillRect(140,100,100,100);
        c.beginPath();
        c.arc(300,100,40,0,2*Math.PI);
        c.strokeStyle="pink";
        c.stroke();
        c.beginPath();
        c.moveTo(70,300);
        c.lineTo(300,200);
        c.strokeStyle="green";
        c.stroke();
    </script>
</body>
</html>
